<?xml version="1.0" encoding="UTF-8"?>
<tests name="Modify element" template="test.tpl.xslt" templatingType="smarty">
	<xml>
		<file>oneElement.xml</file>
		<file>twoElements.xml</file>
		<generator name="testGenerator" output="manyElements.xml">
			<setting name="elementNumber">20</setting>
		</generator>
	</xml>
	<test name="Rename">
		<setting name="action">rename</setting>
		<setting name="newName">newTestName</setting>
	</test>
	<test name="Remove">
		<setting name="action">remove</setting>
	</test>
</tests>
